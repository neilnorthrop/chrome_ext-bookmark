{
  "manifest_version": 2,

  "name": "Bookmark",
  "description": "This extension saves your bookmarks to a website to list out here.",

  "version": "0.1",

  "permissions": [
    "http://savedsites.herokuapp.com/"
  ],

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  }
}
